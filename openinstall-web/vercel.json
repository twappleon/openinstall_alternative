{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/downloads/(.*)",
      "dest": "/downloads/$1",
      "headers": {
        "Content-Type": "application/vnd.android.package-archive"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

