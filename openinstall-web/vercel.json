{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/downloads/(.*)",
      "destination": "/downloads/$1"
    }
  ],
  "headers": [
    {
      "source": "/downloads/(.*\\.apk)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        }
      ]
    }
  ]
}

